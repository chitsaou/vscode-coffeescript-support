{
  "name": "coffeescript-support",
  "description": "Language Server for Advanced CoffeeScript Support",
  "version": "0.0.1",
  "author": "Yucheng Chuang",
  "license": "MIT",
  "engines": {
    "node": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chitsaou/vscode-coffeescript-support"
  },
  "dependencies": {
    "coffeescript": "^2.1.0",
    "glob": "^7.1.2",
    "lokijs": "^1.5.1",
    "vscode-languageserver": "^3.4.2"
  },
  "scripts": {
    "installServer": "installServerIntoExtension ../client ./package.json ./tsconfig.json",
    "compile": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc -p .",
    "watch": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc -w -p .",
    "test": "mocha"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/glob": "^5.0.34",
    "@types/lokijs": "^1.5.0",
    "@types/mocha": "^2.2.44",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "ts-node": "^3.3.0"
  }
}
