{
  "name": "coffeescript-support",
  "description": "Advanced CoffeeScript Support for VSCode",
  "author": "Yucheng Chuang",
  "license": "MIT",
  "version": "0.2.0-beta.5",
  "publisher": "yorkxin",
  "repository": {
    "type": "git",
    "url": "https://github.com/chitsaou/vscode-coffeescript-support"
  },
  "engines": {
    "vscode": "^1.26.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "icon": "assets/icon.png",
  "activationEvents": [
    "onLanguage:coffeescript",
    "workspaceContains:**/*.coffee"
  ],
  "main": "./out/src/extension",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "update-vscode": "node ./node_modules/vscode/bin/install",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "dependencies": {
    "vscode": "^1.1.21",
    "vscode-languageclient": "^5.0.0"
  }
}
